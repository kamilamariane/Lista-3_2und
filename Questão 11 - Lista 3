#include <stdio.h>
#include <stdlib.h>

//questao 11

int main(){
int x; //numero lido
int i;
int primo;
primo=1; //supondo q seja primo

printf("Digite um numero\n"); scanf("%d", &x);
for(i=2; i<=x-1 ; i++){
  if( x%i == 0){
     primo=0;
  }
 }
 
if(primo==0){
  printf("%d não é primo", x);
}
else{
  printf("%d é primo", x);
}
 return 0;
}
