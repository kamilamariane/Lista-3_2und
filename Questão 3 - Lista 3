#include<stdio.h>

int main(){  
    int divfour=0; 
    int x; //contador 
    
    /*logica:4x<100
    x<100/4
    x<25
    x=24*/
    printf ("Os numeros divisiveis por 4 menores que 100 sÃ£o:\n");
    
    for(x=1; x<=24; x++){
        divfour = x * 4;
        printf(" %d\n", divfour);
    }
    
    return 0;
}
