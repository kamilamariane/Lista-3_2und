
#include <stdio.h>
#include <stdlib.h>

int main(){
    int numero[10]; //entradas do usuario
    int i; //contador
    
    printf ("digite 10 numeros:\n");
    for (i = 0; i <10; i++){
        scanf("%d", &numero[i]);
    }
    i = 0;
    
    printf("Os numeros no intervalo [10,50] sÃ£o:\n");
    while(i<10){ 
        if(numero[i]<=50 && numero[i]>=10){
            printf("%d\n", numero[i]);  
        }
        i++;
    }
    return 0;
}
